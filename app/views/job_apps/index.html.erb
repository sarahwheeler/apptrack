<h1>Listing job_apps</h1>

<table>
  <thead>
    <tr>
      <th>Link</th>
      <th>Date saved</th>
      <th>Date applied</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_apps.each do |job_app| %>
      <tr>
        <td><%= job_app.link %></td>
        <td><%= job_app.date_saved %></td>
        <td><%= job_app.date_applied %></td>
        <td><%= link_to 'Show', job_app %></td>
        <td><%= link_to 'Edit', edit_job_app_path(job_app) %></td>
        <td><%= link_to 'Destroy', job_app, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job app', new_job_app_path %>
